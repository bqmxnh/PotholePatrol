<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:id="@+id/rhzrgg7yhfz"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@drawable/_bg__privacy_ek2_shape"
        android:clipToPadding="false"
        android:paddingTop="2dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <RelativeLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                xmlns:tools="http://schemas.android.com/tools"
                android:id="@+id/roq36dxkes3r_rl"
                android:layout_width="match_parent"
                android:layout_height="263dp"
                android:layout_marginHorizontal="1dp"
                android:layout_marginBottom="0dp">

                <ImageView
                    android:id="@+id/roq36dxkes3r"
                    android:layout_width="match_parent"
                    android:layout_height="263dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/image_4" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:paddingTop="30dp"
                    android:paddingBottom="0dp">

                    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        xmlns:tools="http://schemas.android.com/tools"
                        android:id="@+id/raxv1ja7pna"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="24dp"
                        android:layout_marginBottom="34dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/r1oa4o0h5hsq"
                            android:layout_width="28dp"
                            android:layout_height="28dp"
                            android:layout_marginEnd="100dp"
                            android:scaleType="fitXY"
                            android:src="@drawable/right_arrow" />

                        <TextView
                            android:id="@+id/r83hggcuf0ri"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="Privacy"
                            android:textColor="#0D1B34"
                            android:textSize="24sp" />
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/r6dgd4gf9f4g"
                        android:layout_width="134dp"
                        android:layout_height="120dp"
                        android:layout_marginHorizontal="131dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/image_privacy" />
                </LinearLayout>
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/rvtpy6fszffi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="26dp"
                android:layout_marginBottom="29dp"
                android:background="@drawable/rectangle_3_shape"
                android:elevation="10dp"
                android:orientation="vertical"
                android:paddingHorizontal="20dp"
                android:paddingTop="14dp"
                android:paddingBottom="32dp">

                <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    xmlns:tools="http://schemas.android.com/tools"
                    android:id="@+id/rdihecmyumml"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="19dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/r5bmwnr1ufqb"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/shared_folder" />

                    <View
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/rdsxy3foix0j"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Data Sharing Options"
                        android:textColor="#0D1B34"
                        android:textSize="25sp" />
                </LinearLayout>


                <Button
                    android:id="@+id/button_save_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="0dp"
                    android:backgroundTint="#F5F5F5"
                    android:elevation="20dp"
                    android:gravity=""
                    android:paddingVertical="19dp"
                    android:text="On"
                    android:textColor="#626262"
                    android:textSize="25dp"
                    app:cornerRadius="15dp" />

                <Button
                    android:id="@+id/button_save_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="0dp"
                    android:backgroundTint="#F5F5F5"
                    android:elevation="20dp"
                    android:gravity=""
                    android:paddingVertical="19dp"
                    android:text="Off"
                    android:textColor="#626262"
                    android:textSize="25dp"
                    app:cornerRadius="15dp" />
            </LinearLayout>

            <Button
                android:id="@+id/roduvdc3iiqe"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="30dp"
                android:backgroundTint="#AFC7B1"
                android:elevation="20dp"
                android:gravity="center_horizontal"
                android:paddingVertical="19dp"
                android:text="Save"
                android:textColor="#FFFFFF"
                android:textSize="25dp"
                app:cornerRadius="15dp" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>