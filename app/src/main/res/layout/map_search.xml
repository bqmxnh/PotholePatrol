<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/ic_close"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginBottom="12dp"
        android:layout_marginHorizontal="181dp"
        android:src="@mipmap/ic_close"
        android:scaleType="fitXY"
        />
    <LinearLayout
        android:id="@+id/search_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background_search"
        android:paddingVertical="41dp"
        android:orientation="vertical">
        <TextView
            android:id="@+id/title_location"
            android:textColor="#000000"
            android:textSize="24sp"
            android:layout_marginBottom="14dp"
            android:layout_marginStart="33dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="Location"
            />
        <LinearLayout
            android:id="@+id/address_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/search_frame"
            android:paddingHorizontal="19dp"
            android:layout_marginBottom="16dp"
            android:layout_marginHorizontal="30dp"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/search_frame"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingVertical="6dp"
                android:paddingHorizontal="0dp"
                android:layout_marginTop="6dp"
                android:orientation="vertical"
                >
                <TextView
                    android:id="@+id/title_address"
                    android:textColor="#616161"
                    android:textSize="15sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Your address"
                    />
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:id="@+id/title_details"
            android:textColor="#000000"
            android:textSize="24sp"
            android:layout_marginBottom="16dp"
            android:layout_marginStart="32dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="Details"
            />
        <TextView
            android:id="@+id/title_dimension"
            android:textColor="#000000"
            android:textSize="16sp"
            android:layout_marginBottom="11dp"
            android:layout_marginStart="32dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="Dimension"
            />
        <LinearLayout
            android:id="@+id/dimension_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:layout_marginBottom="16dp"
            android:layout_marginHorizontal="30dp"
            android:orientation="horizontal">
            <LinearLayout
                android:id="@+id/compact_container"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/search_frame"
                android:paddingVertical="11dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/title_compact"
                    android:textColor="#AFC7B1"
                    android:textSize="15sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Compact"
                    />
            </LinearLayout>
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"
                />
            <LinearLayout
                android:id="@+id/average_container"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/search_frame"
                android:paddingVertical="11dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/title_average"
                    android:textColor="#AFC7B1"
                    android:textSize="15sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Average"
                    />
            </LinearLayout>
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"
                />
            <LinearLayout
                android:id="@+id/large_container"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/search_frame"
                android:paddingVertical="11dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/title_large"
                    android:textColor="#AFC7B1"
                    android:textSize="15sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Large"
                    />
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:id="@+id/title_depth"
            android:textColor="#000000"
            android:textSize="16sp"
            android:layout_marginBottom="11dp"
            android:layout_marginStart="32dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="Depth"
            />
        <LinearLayout
            android:id="@+id/depth_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:layout_marginBottom="18dp"
            android:layout_marginHorizontal="30dp"
            android:orientation="horizontal">
            <LinearLayout
                android:id="@+id/shallow_container"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/search_frame"
                android:paddingVertical="11dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/title_shallow"
                    android:textColor="#AFC7B1"
                    android:textSize="15sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Shallow"
                    />
            </LinearLayout>
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"
                />
            <LinearLayout
                android:id="@+id/noticeable_container"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/search_frame"
                android:paddingVertical="11dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/title_noticeable"
                    android:textColor="#AFC7B1"
                    android:textSize="15sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Noticeable"
                    />
            </LinearLayout>
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"
                />
            <LinearLayout
                android:id="@+id/deep_container"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/search_frame"
                android:paddingVertical="11dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/title_deep"
                    android:textColor="#AFC7B1"
                    android:textSize="15sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Deep"
                    />
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:id="@+id/title_shape"
            android:textColor="#000000"
            android:textSize="16sp"
            android:layout_marginBottom="11dp"
            android:layout_marginStart="31dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="Shape"
            />
        <LinearLayout
            android:id="@+id/shape_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:layout_marginBottom="19dp"
            android:layout_marginHorizontal="30dp"
            android:orientation="horizontal">
            <LinearLayout
                android:id="@+id/uniform_container"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/search_frame"
                android:paddingVertical="11dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/title_uniform"
                    android:textColor="#AFC7B1"
                    android:textSize="15sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Uniform"
                    />
            </LinearLayout>
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"
                />
            <LinearLayout
                android:id="@+id/uneven_container"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/search_frame"
                android:paddingVertical="11dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/title_uneven"
                    android:textColor="#AFC7B1"
                    android:textSize="15sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Uneven"
                    />
            </LinearLayout>
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"
                />
            <LinearLayout
                android:id="@+id/jagged_container"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/search_frame"
                android:paddingVertical="11dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/title_jagged"
                    android:textColor="#AFC7B1"
                    android:textSize="15sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Jagged"
                    />
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:id="@+id/title_severity_level"
            android:textColor="#000000"
            android:textSize="16sp"
            android:layout_marginBottom="16dp"
            android:layout_marginStart="31dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="Severity level"
            />
        <RelativeLayout
            android:id="@+id/level_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            android:layout_marginHorizontal="36dp">

            <ImageView
                android:id="@+id/ic_bar"
                android:layout_width="match_parent"
                android:layout_height="12dp"
                android:background="@drawable/level_bar"
                android:layout_centerInParent="true"/>

            <ImageView
                android:id="@+id/ic_level"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@mipmap/ic_level"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
        <LinearLayout
            android:id="@+id/title_level"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:layout_marginBottom="33dp"
            android:layout_marginHorizontal="46dp"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/title_low"
                android:textColor="#000000"
                android:textSize="16sp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Low"
                />
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"
                />
            <TextView
                android:id="@+id/title_medium"
                android:textColor="#000000"
                android:textSize="16sp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Medium"
                />
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"
                />
            <TextView
                android:id="@+id/title_high"
                android:textColor="#000000"
                android:textSize="16sp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="High"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/btn_confirm"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:background="@drawable/btn_frame"
            android:paddingVertical="20dp"
            android:layout_marginHorizontal="32dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/title_confi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Confirm"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:textStyle="bold" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>