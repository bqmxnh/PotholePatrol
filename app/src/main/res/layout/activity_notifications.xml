<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:clipToPadding="false"
        android:id="@+id/rj0qch61d8sb"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@drawable/_bg__notifications_ek2_shape"
        android:paddingTop="2dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <RelativeLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/r90zz0iwo19s_rl"
                android:layout_width="match_parent"
                android:layout_height="263dp"
                android:layout_marginBottom="0dp"
                android:layout_marginHorizontal="2dp"
                >
                <ImageView
                    android:id="@+id/r90zz0iwo19s"
                    android:layout_width="match_parent"
                    android:layout_height="263dp"
                    android:src="@drawable/image_4"
                    android:scaleType="fitXY"
                    />
                <LinearLayout
                    android:paddingTop="20dp"
                    android:paddingBottom="32dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:tools="http://schemas.android.com/tools"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/r3pyjmtgv006"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:layout_marginBottom="30dp"
                        android:layout_marginHorizontal="24dp"
                        android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/r3x3mncglvqj"
                            android:layout_width="28dp"
                            android:layout_height="28dp"
                            android:layout_marginEnd="70dp"
                            android:src="@drawable/right_arrow"
                            android:scaleType="fitXY"
                            />
                        <TextView
                            android:id="@+id/rososympjmy"
                            android:textColor="#0D1B34"
                            android:textSize="24sp"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:text="Notifications"
                            />
                    </LinearLayout>
                    <ImageView
                        android:id="@+id/rihtawjmmdnk"
                        android:layout_width="match_parent"
                        android:layout_height="121dp"
                        android:layout_marginHorizontal="142dp"
                        android:src="@drawable/image_tb"
                        android:scaleType="fitXY"
                        />
                </LinearLayout>
            </RelativeLayout>
            <LinearLayout
                android:id="@+id/rdcuu2nxw7sm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/rectangle_3_shape"
                android:paddingTop="18dp"
                android:paddingBottom="32dp"
                android:paddingHorizontal="20dp"
                android:layout_marginBottom="28dp"
                android:layout_marginHorizontal="26dp"
                android:elevation="10dp"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/rzvfsmslvakd"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_marginBottom="18dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/rxqk6bpg79h8"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginEnd="33dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/warning" />

                    <TextView
                        android:id="@+id/romfrp9f65r"
                        android:textColor="#0D1B34"
                        android:textSize="25sp"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="Alert Preferences"
                        />
                </LinearLayout>
                <Button
                    android:id="@+id/button_sound"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="0dp"
                    android:backgroundTint="#F5F5F5"
                    android:elevation="20dp"
                    android:gravity=""
                    android:paddingVertical="19dp"
                    android:text="Sound"
                    android:textColor="#626262"
                    android:textSize="25dp"
                    app:cornerRadius="15dp" />
                <Button
                    android:id="@+id/button_vibrate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="0dp"
                    android:backgroundTint="#F5F5F5"
                    android:elevation="20dp"
                    android:gravity=""
                    android:paddingVertical="19dp"
                    android:text="Vibrate"
                    android:textColor="#626262"
                    android:textSize="25dp"
                    app:cornerRadius="15dp" />
                </LinearLayout>
            <Button
                android:id="@+id/roduvdc3iiqe"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="30dp"
                android:backgroundTint="#AFC7B1"
                android:elevation="20dp"
                android:gravity="center_horizontal"
                android:paddingVertical="19dp"
                android:text="Save"
                android:textColor="#FFFFFF"
                android:textSize="25dp"
                app:cornerRadius="15dp" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>